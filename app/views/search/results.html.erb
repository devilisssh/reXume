<div class="row" style="padding-top: 2em;">
  <% @data.each do |user| %>
    <div class="col m6">
      <div class="card transparent">
        <div class="card-content">
          <span class="card-title"><%= user.name %></span>
          <p><%= user.resume.summary %></p>
        </div>
        <div class="card-action">
          <%= link_to 'Download Resume', resume_path(user.resume, format: 'pdf')%>
          <%= link_to 'View Resume', resume_path(user.resume) %>
        </div>
      </div>
    </div>
  <% end %>
</div>